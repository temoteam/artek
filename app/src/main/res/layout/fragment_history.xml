    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.artek.app.main.HistoryFragment">

    <WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/mapWebView"
